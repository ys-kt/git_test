<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ys.entity">
	<!-- 配置类和表 -->
	<class name="Backend" table="backend">
		<!-- 配置主键 -->
		<id name="backendId" column="backend_id">
			<!-- 配置主键生成策略 -->
			<generator class="native"></generator>
		</id>
		
		<!-- 配置其他普通属性 -->
		<property name="backendUsername" column="articles_name"></property>
		<property name="backendPwd" column="backend_pwd"></property>
		<property name="backendLevel" column="backend_level"></property>
	</class>
</hibernate-mapping>
